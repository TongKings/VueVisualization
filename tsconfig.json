{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@/stores*": [
        "./src/stores*"
      ],
      "@/components*": [
        "./src/components*"
      ],
        "@/modules*": [
       "./src/modules*"
      ],
       "@/utils*": [
        "./src/utils*"
      ],
      "@/views*": [
        "./src/views*"
      ],
      "@/assets*": [
        "./src/assets*"
      ]
  }
},
"include": [
  "src/**/*.ts",
  "src/**/*.d.ts",
  "src/**/*.tsx",
  "src/**/*.vue"
],
"references": [
  {
    "path": "./tsconfig.node.json"
  }
]
}